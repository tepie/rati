<?xml version="1.0" encoding="UTF-8"?><xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">	<xs:element name="abinitio">		<xs:complexType>			<xs:sequence>				<xs:element name="datastore" type="datastoreType"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:complexType name="annotationType">		<xs:simpleContent>			<xs:extension base="xs:string">				<xs:attribute name="name" use="required">					<xs:simpleType>						<xs:restriction base="xs:NMTOKEN">							<xs:enumeration value="comment"/>							<xs:enumeration value="created_by"/>							<xs:enumeration value="creation_date"/>							<xs:enumeration value="description"/>							<xs:enumeration value="display_name"/>							<xs:enumeration value="member_locations"/>							<xs:enumeration value="modification_date"/>							<xs:enumeration value="modified_by"/>							<xs:enumeration value="name"/>							<xs:enumeration value="path"/>						</xs:restriction>					</xs:simpleType>				</xs:attribute>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<xs:complexType name="datastoreType">		<xs:sequence>			<xs:element name="object" type="objectType" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="objectType">		<xs:choice maxOccurs="unbounded">			<xs:element name="reference" type="referenceType"/>			<xs:element name="annotation" type="annotationType"/>		</xs:choice>		<xs:attribute name="category" type="xs:string" use="required"/>		<xs:attribute name="oid" type="xs:string" use="required"/>	</xs:complexType>	<xs:complexType name="referenceType">		<xs:attribute name="name" use="required">			<xs:simpleType>				<xs:restriction base="xs:NMTOKEN">					<xs:enumeration value="arulesOID"/>					<xs:enumeration value="categoryOID"/>					<xs:enumeration value="parentOID"/>				</xs:restriction>			</xs:simpleType>		</xs:attribute>		<xs:attribute name="oidref" type="xs:string" use="required"/>	</xs:complexType></xs:schema>